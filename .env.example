# Backend Configuration
PORT=3001
NODE_ENV=development

# Frontend URL (for CORS) - Railway will set this
FRONTEND_URL=http://localhost:3000

# Production Frontend URL (set this in Railway dashboard)
# FRONTEND_URL=https://your-app-name.vercel.app

# Logging Level
LOG_LEVEL=info

# Security Configuration (optional - your code has defaults)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100